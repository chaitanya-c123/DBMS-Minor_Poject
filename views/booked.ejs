<%- include('partials/header') %>
<br>
<form action="/back/home" method="post">
  <div class="text-left ">
    <button type="submit" name="signout"  class="signout btn btn-success"><--BACK</button></div>
  </form>  
 <div class="box text-center container" id="heading">
   <br>
 <h1 class="border">BOOKED VEHICLE LIST</h1>
  <% if(newListItems.length==0){%>
     <h3>There are no current booked vehicles</h3>
    <%}else{%>
     
      <br><br>
    </div>
      <div class="box container">
        <table class="table table-hover">
        <thead class="table-head">
          <tr>
            <th scope="col">#</th>
            <th scope="col">Model Name</th>
            <th scope="col">Car Year</th>
            <th scope="col">Car Number</th>
            <th scope="col">Car Transmission</th>
            <th scope="col">Price</th>
          </tr>
        </thead>
        <% for(var i=0;i<newListItems.length;i++){ %>
          <div class="item">
                  <tbody>
                    <tr>
                      <th scope="row"><%=i+1%></th>
                      <td><%=newListItems[i].modelName%></td>
                      <td><%=newListItems[i].carYear%></td>
                      <td><%=newListItems[i].carNo%></td>
                      <td><%=newListItems[i].carTrans%></td>
                      <td><%=newListItems[i].carRate%></td>
                      
                    </tr>
               
                 
        </div>
   <%} %>
  </table>
<%}%>              
    </div>
    
    </body>
</html>