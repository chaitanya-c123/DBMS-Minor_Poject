      </span><br>
      <span>
        <label style="padding-left: 30px;">Password</label>
        <input type="password" class="form-control" id="password" placeholder="password" name="password" required="true">
      </span><br>
      <span>
        <label style="padding-left: 30px;">Name</label>
        <input type="string" class="form-control" id="name" placeholder="name" name="name" required="true">
      </span><br>
      <span>
        <label style="padding-left: 30px;">Adress</label>
        <input type="string" class="form-control" placeholder="address" name="address" required="true">
      </span><br>
      <span>
        <label style="padding-left: 30px;">Phone Number</label>
        <input type="string" class="form-control" placeholder="phone" name="pno" required="true">
      </span><br>
      
      <span>
        <label style="padding-left: 30px;">Identification</label></b>
        <input type="file" name="filei">
      </span>
      <button style="margin-left: 30px;" type="submit" class="btn btn-dark">Register</button>
     </form>
    </div>
  
  
  
  
  
  
  
