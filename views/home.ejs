<%- include('partials/header') %>
      <div id="customerimg" class="container-fluid">
        <h1 class="main-text">VBC RENTALS</h1>
      </div>
      <br><br>
      <section class="section-3">
        <div class="container text-center">
          <div class="row">
            <div class="col-md-3 col-sm-12">
              <form class="item " action="/addPage" method="post">
                <button type="submit" class="btn btn-primary " name="list" value=""><b>Add Vehicles</b></button>
              </form>
            </div>
            <div class="col-md-3 col-sm-12">
              <form class="item2" action="/book" method="post">
                <button type="submit" class="btn btn-primary " name="blist" value=""><b>Booked vehicles</b></button>
              </form>
            </div>
            <div class="col-md-3 col-sm-12">
              <form action="/users" method="post">
                <button type="submit" class="btn btn-primary" ><b>Booking Details</b></button>
              </form>
            </div>
            <div class="col-md-3 col-sm-12">
              <button type="submit"  class="btn btn-primary" name="blist" value=""><a class="button-link" href="/queryPage"><b>Customer Queries</b></a></button>
            </div>
          </div>
        </div>
      </section><br><br>
      <div class="container">
        <div class="box border" id="heading">
          <div class="box heading">
            <h1>Vehicle List</h1>
          </div>
        </div>
      <br>
        
    <%if(newListItems.length==0){%>
      <h3>OOPS! Currently No vehicles Available</h3>
    <%}else{%>
  <div class="container">
  <div class="row">
    <% for(var i=0;i<newListItems.length;i++){ %>
                 <div class="col-md-3 border text-center">
                   <img style="max-height:300px;max-width:300px" src="/upload/<%=newListItems[i].image%>"><br>
                  <b><span style="font-size: large;color:brown">ModelName </span>: <%=newListItems[i].modelName%></b><br>
                  <b><span style="font-size: large;color:brown">Car Year</span> : <%=newListItems[i].carYear%></b><br>
                  <b><span style="font-size: large;color:brown">Car Number</span> : <%=newListItems[i].carNo%></b><br>
                  <b><span style="font-size: large;color:brown">Car Transmission</span> : <%=newListItems[i].carTrans%></b><br>
                  <b><span style="font-size: large;color:brown">Car Price per Day</span> : <%=newListItems[i].carRate%></b><br><br>
                  <form action="/delete" method="post">
                    <button type="submit" name="deleted" style="background-color:#AE431E;color:white" value="<%=newListItems[i]._id%>"><b>Delete</b></button>
                   </form>
              </div>
              <%}}%>
  </div>
</div>
    <br>
    <br><br>
    <form action="/logouta" method="post">
      <div class="text-center">
      <button type="submit" name="logout" class="logout btn btn-danger">Log out</button><br><br>
    </div>
    </form>
    </div>
    </div>
  
    </body>
</html>
<%- include('partials/footer') %>